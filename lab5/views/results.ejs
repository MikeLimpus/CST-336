<%- include("partials/header.ejs") %> 
<body>
    <h1>Quotes Found</h1>
    <% for (i of quotes) { %>
        <%= i.quote %><br>
        -<em>
            <a href="#" id="<%=i.authorId%>">
                <%=i.firstName%> <%=i.lastName%>
            </a>
        </em><br><br>
        <% } %>
        <div id="authorInfo"></div>
        <%- include("partials/footer.ejs")%>
        <script src="/js/scripts.js"></script>
</body>
</html>